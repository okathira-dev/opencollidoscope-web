{
  "name": "client-web-api-sandbox",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=22.12.0"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --cache \"src/**/*\"",
    "lint:fix": "eslint --cache \"src/**/*\" --fix",
    "format": "prettier --cache --check \"./src\"",
    "format:fix": "prettier --cache --write \"./src\"",
    "markup": "markuplint \"src/**/*.{jsx,tsx,html}\"",
    "markup:fix": "markuplint \"src/**/*.{jsx,tsx,html}\" --fix",
    "check": "npm run markup && npm run lint && npm run format",
    "check:fix": "npm run markup:fix && npm run lint:fix && npm run format:fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.3.1",
    "@mui/material": "^6.4.5",
    "jotai": "^2.12.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tone": "^15.0.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@markuplint/jsx-parser": "^4.7.16",
    "@markuplint/react-spec": "^4.5.14",
    "@types/eslint__js": "^8.42.3",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/parser": "^8.29.1",
    "@vitejs/plugin-react-swc": "^3.8.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^4.4.2",
    "eslint-plugin-html": "^8.0.0",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.3",
    "markuplint": "^4.11.4",
    "prettier": "^3.5.2",
    "ts-jest": "^29.2.6",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0",
    "vite": "^6.2.6"
  },
  "lint-staged": {
    "*.{jsx,tsx,html}": [
      "markuplint"
    ],
    "src/**/*": [
      "eslint --cache",
      "prettier --check"
    ]
  }
}
